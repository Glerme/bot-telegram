generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String      @id
  name      String
  createdAt DateTime?   @default(now())
  chatId    String      @unique
  userName  String
  trackings Trackings[]
}

model Trackings {
  id        String    @id
  codigo    String
  descricao String?
  createdAt DateTime? @default(now())

  User   User   @relation(fields: [userId], references: [id])
  userId String
}
